{
    "name": "cronpulse/laravel-monitor",
    "description": "A cronjob monitoring library for Laravel using Cronpulse.live",
    "authors": [
        {
            "name": "Cronpulse.live",
            "email": "team@cronpulse.live"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Cronpulse\\LaravelMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cronpulse\\LaravelMonitor\\MonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}